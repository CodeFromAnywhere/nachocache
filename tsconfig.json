{
  "include": ["**/*.ts"],
  "files": [
    "custom.d.ts",
    "worker-configuration.d.ts",
    ".wrangler/types/runtime.d.ts"
  ],
  "compilerOptions": {
    "resolveJsonModule": true,
    "module": "NodeNext",
    "moduleResolution": "NodeNext",
    "lib": ["es2022", "DOM"],
    "strict": true,
    "noEmit": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "declaration": true
  }
}
